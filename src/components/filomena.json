   },
   {
      "id":22,
      "type":"text",
      "page":1,
      "text":"Ellas andan embriagadas con esa nueva libertad.",
      "className":"grs-5 gre-6 gcs-3 gce-7 center",
      "delay":3000,
      "renderNext":true,
      "url":"",
      "nextPage":false
   },
   {
